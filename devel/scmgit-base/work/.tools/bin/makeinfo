#!/bin/sh
wrapperlog="${TOOLS_WRAPPER_LOG-/boot/develop/hpkgsrc/devel/scmgit-base/work/.work.log}"
echo "[*] "/boot/develop/hpkgsrc/devel/scmgit-base/work/.tools/bin/makeinfo" $@" >> $wrapperlog
echo "<.> for arg in "$@"; do case "$arg" in --version) exit 1 ;; esac; done; /boot/develop/hpkgsrc/mk/gnu-config/missing makeinfo "$@"" >> $wrapperlog
for arg in "$@"; do case "$arg" in --version) exit 1 ;; esac; done; /boot/develop/hpkgsrc/mk/gnu-config/missing makeinfo "$@"
