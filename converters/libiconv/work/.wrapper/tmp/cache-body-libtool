--mode=compile) arg=--mode=compile; split_arg=no; cachehit=yes ;;
-I.) arg=-I.; split_arg=no; cachehit=yes ;;
-I.) arg=-I.; split_arg=no; cachehit=yes ;;
-I..) arg=-I..; split_arg=no; cachehit=yes ;;
-I./..) arg=-I./..; split_arg=no; cachehit=yes ;;
-I../include) arg=-I../include; split_arg=no; cachehit=yes ;;
-Wno-error) arg=-Wno-error; split_arg=no; cachehit=yes ;;
-fvisibility=hidden) arg=-fvisibility=hidden; split_arg=no; cachehit=yes ;;
-DLIBDIR=\"/boot/common/pkg/lib\") arg=-DLIBDIR=\"/boot/common/pkg/lib\"; split_arg=no; cachehit=yes ;;
-DBUILDING_LIBCHARSET) arg=-DBUILDING_LIBCHARSET; split_arg=no; cachehit=yes ;;
-DBUILDING_DLL) arg=-DBUILDING_DLL; split_arg=no; cachehit=yes ;;
-DENABLE_RELOCATABLE=1) arg=-DENABLE_RELOCATABLE=1; split_arg=no; cachehit=yes ;;
-DIN_LIBRARY) arg=-DIN_LIBRARY; split_arg=no; cachehit=yes ;;
-DINSTALLDIR=\"/boot/common/pkg/lib\") arg=-DINSTALLDIR=\"/boot/common/pkg/lib\"; split_arg=no; cachehit=yes ;;
-DNO_XMALLOC) arg=-DNO_XMALLOC; split_arg=no; cachehit=yes ;;
-Dset_relocation_prefix=libcharset_set_relocation_prefix) arg=-Dset_relocation_prefix=libcharset_set_relocation_prefix; split_arg=no; cachehit=yes ;;
-Drelocate=libcharset_relocate) arg=-Drelocate=libcharset_relocate; split_arg=no; cachehit=yes ;;
-DHAVE_CONFIG_H) arg=-DHAVE_CONFIG_H; split_arg=no; cachehit=yes ;;
-c) arg=-c; split_arg=no; cachehit=yes ;;
